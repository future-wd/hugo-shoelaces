{
  "description": "Hugo starter theme for building modern websites with bootstrap that are secure, fast, and SEO-ready â€” by default.",
  "version": "1.6.0",
  "repository": "https://github.com/future-wd/hugo-shoelaces",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "Explorer >= 11"
  ],
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --gc",
    "dev:preview": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --gc --buildDrafts --buildFuture",
    "build": "exec-bin node_modules/.bin/hugo/hugo --gc --minify --cleanDestinationDir",
    "start": "hugo server --disableFastRender --renderToDisk --cleanDestinationDir",
    "start:preview": "hugo server --renderToDisk --cleanDestinationDir --disableFastRender --buildDrafts --buildFuture ",
    "start:production": "exec-bin node_modules/.bin/hugo/hugo server --renderToDisk --cleanDestinationDir --disableFastRender --gc --minify --environment production",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles && npm run -s lint:markdown",
    "lint:scripts": "eslint assets/js config functions",
    "lint:styles": "stylelint \"assets/scss/**/*.{css,sass,scss,sss,less}\"",
    "lint:markdown": "markdownlint *.md content/**/*.md",
    "npm-check": "npx npm-check -u",
    "hugo-version": "exec-bin node_modules/.bin/hugo/hugo version",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "markdownlint-cli": "^0.29.0",
    "npm-check": "^5.9.2",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13",
    "stylelint-config-standard": "^22.0"
  },
  "otherDependencies": {
    "hugo": "0.89.0"
  },
  "dependencies": {
    "@babel/cli": "^7.15",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@fullhuman/postcss-purgecss": "^4.0",
    "@popperjs/core": "^2.10.2",
    "autoprefixer": "^10.3.7",
    "bootstrap": "^5.1.3",
    "core-js": "^3.18.3",
    "exec-bin": "^1.0.0",
    "hugo-installer": "^3.1",
    "lazysizes": "^5.3.2",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "purgecss-whitelister": "^2.4",
    "quicklink": "^2.2.0",
    "regenerator-runtime": "^0.13.9",
    "shx": "^0.3.3"
  }
}
